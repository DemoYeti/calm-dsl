{%- macro patch_field_ahv_disk(obj) %}
{% if obj.operation == "delete" %}PatchField.Ahv.Disks.{{obj.operation}}(index={{obj.index}}){% endif %}
{% if obj.operation == "modify" %}PatchField.Ahv.Disks.{{obj.operation}}(index={{obj.index}}, editable={{obj.editable}}, operation="{{obj.operation}}"{% if obj.value %}, value='{{obj.value}}'{% endif %}{% if obj.max_val %}, max_val={{obj.max_val}}{% endif %}{% if obj.min_val %}, min_val={{obj.min_val}}{% endif %}){% endif %}
{% if obj.operation == "add" %}PatchField.Ahv.Disks.{{obj.operation}}({{obj.disk_data}}){% endif %}
{%- endmacro %}

{{ patch_field_ahv_disk(obj) }}
